    "start": "react-scripts start",
    "test": "react-scripts test",
    "test:nowatch": "react-scripts test --watchAll=false",
    "stylelint:check-conflicts": "stylelint-config-prettier-check"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 2 chrome versions"
    ]
  },
