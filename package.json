  "scripts": {
    "build": "react-scripts build",
    "build:profile": "npm run build -- --profile",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore --ignore-path .eslintignore",
    "lint:fix": "npm run lint -- --fix",
    "lint:scss": "stylelint --syntax scss 'src/**/*.scss' --ignore-path .gitignore",
    "lint:scss:fix": "npm run lint:scss -- --fix",
    "postinstall": "husky install",
    "postpublish": "pinst --enable",
    "predeploy": "npm run build",
    "prepublishOnly": "pinst --disable",
    "prettier:check": "prettier --check 'src/**/*.{js,jsx,ts,tsx,css,html,json,md}' --ignore-path .gitignore --ignore-path .eslintignore",
    "prettier:write": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,html,json,md}' --ignore-path .gitignore --ignore-path .eslintignore",
    "start": "react-scripts start",
    "stylelint:check-conflicts": "stylelint-config-prettier-check",
    "test": "react-scripts test",
    "test:coverage": "npm test -- --coverage --watchAll=false",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache --watchAll=false",
    "test:nowatch": "npm test -- --watchAll=false"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 2 chrome versions"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
