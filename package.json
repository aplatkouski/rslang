  "scripts": {
    "build": "react-scripts build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore --ignore-path .eslintignore",
    "lint:fix": "npm run lint -- --fix",
    "lint:scss": "stylelint --syntax scss 'src/**/*.scss' --ignore-path .gitignore",
    "lint:scss:fix": "npm run lint:scss -- --fix",
    "postinstall": "husky install",
    "postpublish": "pinst --enable",
    "prepublishOnly": "pinst --disable",
    "predeploy": "npm run build",
    "prettier:check": "prettier --check 'src/**/*.{js,jsx,ts,tsx,css,html,json,md}' --ignore-path .gitignore --ignore-path .eslintignore",
